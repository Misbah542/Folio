# Development Environment Variables
# This file is used when running npm start

# API URLs (if you have a backend)
REACT_APP_API_URL=http://localhost:5000

# Firebase Configuration (Alternative to hardcoding in firebase-analytics.js)
REACT_APP_FIREBASE_API_KEY=YOUR_DEV_API_KEY
REACT_APP_FIREBASE_AUTH_DOMAIN=YOUR_DEV_AUTH_DOMAIN
REACT_APP_FIREBASE_PROJECT_ID=YOUR_DEV_PROJECT_ID
REACT_APP_FIREBASE_STORAGE_BUCKET=YOUR_DEV_STORAGE_BUCKET
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=YOUR_DEV_MESSAGING_SENDER_ID
REACT_APP_FIREBASE_APP_ID=YOUR_DEV_APP_ID
REACT_APP_FIREBASE_MEASUREMENT_ID=YOUR_DEV_MEASUREMENT_ID

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_HIGH_SCORES_API=false
REACT_APP_ENABLE_MULTIPLAYER=false

# Game Configuration
REACT_APP_MAX_HIGH_SCORES=10
REACT_APP_DEFAULT_SOUND_ENABLED=true

# External URLs
REACT_APP_TERMINAL_URL=http://buildwithmisbah.cc
REACT_APP_PORTFOLIO_URL=http://portfolio.buildwithmisbah.cc

# Development Settings
REACT_APP_DEBUG_MODE=true
REACT_APP_SHOW_FPS=false

# Version
REACT_APP_VERSION=1.0.0-dev